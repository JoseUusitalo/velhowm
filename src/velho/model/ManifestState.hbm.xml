<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="velho.model.ManifestState" table="MANIFEST_STATES">
		<meta attribute="class-description">A state of a Manifest.</meta>

		<id name="databaseID"
			column="manifest_state_id"
			type="int">
			<generator class="velho.controller.database.AssignedIdentifierGenerator" />
		</id>
		
		<property	name="uuid"
					column="uuid"
					type="java.util.UUID"
					unique="true"
					not-null="true" />
		
		<property	name="name"
					column="name"
					type="string"
					unique="true"
					not-null="true" />
	</class>
</hibernate-mapping>
