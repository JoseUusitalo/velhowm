<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="velho.model.ProductType" table="TYPES">
		<meta attribute="class-description">A class representing the type of the product.</meta>
		<id name="databaseID" type="int" column="type_id">
			<generator class="native" />
		</id>
		<property name="name" column="NAME" type="string" unique="true" not-null="true"/>
	</class>
</hibernate-mapping>
